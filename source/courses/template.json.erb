<%
  output = {}
  output[:course] = course_name
  output[:links] = []

  cids = data.courses[course_name].split(',')
  cids.each do |id|
    article = sitemap.where(cid: id.to_i).first
    output[:links] << {
      icon: card_icon(article),
      title: article.title,
      url: article.url,
      summary: article.data.summary
    }
  end
%><%= output.to_json %>