<body class="<%= page_classes %>">

  <div id='error-message'></div>

<div class="course-page">
  <header>
    <div style='float:right'><%= partial "profile" %></div>
    <%= partial "header" %>
  </header>

  <main id='main-content'>
    <%= yield %>
